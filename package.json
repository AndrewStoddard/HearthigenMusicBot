{
    "name": "Hearthigen Music Bot",
    "version": "4.0.7",
    "description": "Hearthigen Music Bot is a fork of Lavamusic by brblacky. This is a fork with custom features made for Hearthigen Discord.",
    "main": "dist/index.js",
    "type": "module",
    "scripts": {
        "start": "npm run build && node dist/index.js",
        "build": "tsc --project tsconfig.json",
        "clean": "rm -rf dist",
        "lint": "eslint . --cache --ext .js,.jsx,.ts,.tsx",
        "lint:fix": "eslint . --fix --cache --ext .js,.jsx,.ts,.tsx",
        "format": "prettier --check .",
        "format:fix": "prettier --write ."
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/AndrewStoddard/HearthigenMusicBot.git"
    },
    "keywords": [
        "discord",
        "music",
        "bot",
        "lavalink",
        "shoukaku",
        "lavamusic",
        "typescript",
        "prisma"
    ],
    "author": "Andrew Stoddard",
    "license": "GPL-3.0",
    "homepage": "https://github.com/AndrewStoddard/HearthigenMusicBot#README.md",
    "devDependencies": {
        "@types/better-sqlite3": "^7.6.9",
        "@types/node": "^20.11.16",
        "@types/signale": "^1.4.7",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "eslint": "^8.56.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-unicorn": "^51.0.0",
        "prettier": "^3.2.5",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.3"
    },
    "dependencies": {
        "anilist-node": "^1.14.1",
        "better-sqlite3": "^9.4.0",
        "discord.js": "^14.14.1",
        "dotenv": "^16.4.1",
        "fast-xml-parser": "^4.3.5",
        "node-cron": "^3.0.3",
        "shoukaku": "^4.0.1",
        "signale": "^1.4.0",
        "tslib": "^2.6.2",
        "undici": "^6.6.1"
    },
    "signale": {
        "displayScope": true,
        "displayBadge": true,
        "displayDate": true,
        "displayFilename": true,
        "displayLabel": true,
        "displayTimestamp": true,
        "underlineLabel": true
    }
}
